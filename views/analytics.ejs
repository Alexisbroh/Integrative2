<section>
  <aside  class="p-4 sm:ml-64">
    <aside   class="p-4 border-2">
       <aside   class="grid grid-cols-3 gap-4 mb-4">
        <section >
          <aside  id="anlayticsCard"  class="card" style="width:fit-content height fit-content ">
            <aside id="body-analytics-card" class="card-body">
              <h5 id="anlayticsCard-tittle" class="card-title">Session statistics</h5>
              <p id="anlayticsCard-text"  class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
              <a  id="button-analytics" href="#" class="btn btn-primary">Show Frequency</a>
              <div>
                <canvas id="ChartFrequency"></canvas>
              </div>
              
              <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
              
              <script>
                const ctx = document.getElementById('ChartFrequency');
              
                new Chart(ctx, {
                  type: 'bar',
                  data: {
                    labels: ['Sessions taken', 'Level', 'Time'],
                    datasets: [{
                      label: 'Sessions, Routine, Minutes',
                      data: [5, 1, 2],
                      borderWidth: 1,
                      borderColor: '#36A2EB',
                      backgroundColor: '#9BD0F5',
                    }]
                  },
                  options: {
                    scales: {
                      y: {
                        beginAtZero: true
                      }
                    }
                  }
                });
              </script>
              
            </aside>
          </aside>
          <aside id="anlayticsCard" class="card" style="width: 20rem height 20rem">
            <aside id="body-analytics-card" class="card-body">
              <h5 id="anlayticsCard-tittle" class="card-title">Time of usage</h5>
              <p id="anlayticsCard-text" class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
              <div>
                <canvas id="ChartUsage"></canvas>
              </div>
              
              <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
              
              <script>
                const time_usage = document.getElementById('ChartUsage');
              
                new Chart(time_usage, {
                  type: 'radar',
                  data: {
                    labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
                    datasets: [{
                      label: '# of Votes',
                      data: [12, 19, 3, 5, 2, 3],
                      borderWidth: 1,
                      borderColor: '#9DB2BF',
                      
                      backgroundColor: '#526D82',
                    }]
                  },
                  options: {
                    scales: {
                      y: {
                        beginAtZero: true
                      }
                    }
                  }
                });
              </script>
              
            </aside>
          </aside>
          <aside  id="anlayticsCard" class="card" style="width: 20rem height 20rem";>
            <aside  id="body-analytics-card" class="card-body">
              <h5 id="anlayticsCard-tittle" class="card-title">Level</h5>
              <p id="anlayticsCard-text" class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
              <a id="button-analytics" href="#" class="btn btn-primary">Show Used Levels</a>
              <div>
                <canvas id="ChartLevel"></canvas>
              </div>
              
              <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
              
              <script>
                const intensity = document.getElementById('ChartLevel');
              
                new Chart(intensity, {
                  type: 'line',
                  data: {
                    labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
                    datasets: [{
                      label: '# of Votes',
                      data: [12, 19, 3, 5, 2, 3],
                      borderWidth: 1,
                      borderColor: '#36A2EB',
                      backgroundColor: '#9BD0F5',
                    }]
                  },
                  options: {
                    scales: {
                      y: {
                        beginAtZero: true,
                        
                      }
                    }
                  }
                });
              </script>
              
            
            
            </aside>     
        </section>
       </aside>
    </aside>
 </aside>
