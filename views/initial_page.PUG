doctype html
doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible', content='IE=edge')
  meta(name='viewport', content='width=device-width, initial-scale=1.0')
  title Initial Page Vibes
  link(href='https://cdn.jsdelivr.net/npm/daisyui@3.0.0/dist/full.css', rel='stylesheet', type='text/css')
  link(rel='stylesheet', href='./stylesheets/styles.css')
  script(src='https://kit.fontawesome.com/eb496ab1a0.js', crossorigin='anonymous')
header
  nav.navbar-father
    .max-w-screen-xl.flex.flex-wrap.items-center.justify-between.mx-auto.p-5
      a.flex.items-center(href='#')
        span.self-center.text-2xl.font-semibold.whitespace-nowrap  Vibes
      button.inline-flex.items-center.p-2.ml-3.text-sm(data-collapse-toggle='navbar-hamburger', type='button', aria-controls='navbar-hamburger', aria-expanded='false')
        span.sr-only Open main menu
        svg.w-6.h-6(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
          path(fill-rule='evenodd', d='M2 4.75A.75.75 0 012.75 4h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 4.75zm0 10.5a.75.75 0 01.75-.75h7.5a.75.75 0 010 1.5h-7.5a.75.75 0 01-.75-.75zM2 10a.75.75 0 01.75-.75h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 10z')
      aside#navbar-hamburger.hidden.w-full
        ul.flex.flex-col.font-medium.mt-4.rounded-lg
          button.inline-flex.items-center.p-2.mt-2.ml-3.text-sm.rounded-lg(class='sm:hidden', data-drawer-target='logo-sidebar', data-drawer-toggle='logo-sidebar', aria-controls='logo-sidebar', type='button')
            span.sr-only Open sidebar
            svg.w-6.h-6(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
              path(clip-rule='evenodd', fill-rule='evenodd', d='M2 4.75A.75.75 0 012.75 4h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 4.75zm0 10.5a.75.75 0 01.75-.75h7.5a.75.75 0 010 1.5h-7.5a.75.75 0 01-.75-.75zM2 10a.75.75 0 01.75-.75h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 10z')
          aside#logo-sidebar.fixed.top-0.left-0.z-40.w-64.h-screen.transition-transform.-translate-x-full.smtranslate-x-0(aria-label='Sidebar')
            aside.h-full.px-3.py-4.overflow-y-auto
              a.flex.items-center.mb-5(class='pl-2.5', href='/initial_page')
                span.self-center.text-xl.font-semibold.whitespace-nowrap  Vibes
              ul.space-y-2.font-medium
                li
                  a.flex.items-center.p-2.rounded-lg(href='/initial_page')
                    svg.w-6.h-6.transition.duration-75(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(d='M2 10a8 8 0 018-8v8h8a8 8 0 11-16 0z')
                      path(d='M12 2.252A8.014 8.014 0 0117.748 8H12V2.252z')
                    span.ml-3 Home
                li
                  a.flex.items-center.p-2(href='/routines')
                    svg.flex-shrink-0.w-6.h-6.transition.duration-75(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(d='M5 3a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2V5a2 2 0 00-2-2H5zM5 11a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2v-2a2 2 0 00-2-2H5zM11 5a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2V5zM11 13a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2v-2z')
                    span.flex-1.ml-3.whitespace-nowrap My Routines
                    span.inline-flex.items-center.justify-center.px-2.ml-3.text-sm.font-medium
                li
                  a.flex.items-center.p-2.rounded-lg(href='/analytics')
                    svg.flex-shrink-0.w-6.h-6.transition.duration-75(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(d='M8.707 7.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l2-2a1 1 0 00-1.414-1.414L11 7.586V3a1 1 0 10-2 0v4.586l-.293-.293z')
                      path(d='M3 5a2 2 0 012-2h1a1 1 0 010 2H5v7h2l1 2h4l1-2h2V5h-1a1 1 0 110-2h1a2 2 0 012 2v10a2 2 0 01-2 2H5a2 2 0 01-2-2V5z')
                    span.flex-1.ml-3.whitespace-nowrap Stats and Analytics
                li
                  a.flex.items-center.p-2(href='/logout')
                    svg.flex-shrink-0.w-6.h-6.transition.duration-75(aria-hidden='true', fill='currentColor', viewbox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z', clip-rule='evenodd')
                    span.flex-1.ml-3.whitespace-nowrap Log Out
section
      aside#main.p-4.mx-80.max-w-full
        aside.p-4.border-2
          aside.grid.grid-cols-3.gap-4.mb-4
            section
              each initialPosts in my-posts
                aside.card(style="width: 20rem; height: 20rem")
                  aside#tarjetaJaen.card-body
                    span#tituloJaen= initialPosts.name_user
                    h5#tituloJaen2.card-title= initialPosts.title
                    p#cuerpoJaen.card-text= initialPosts.post
                    p#date= initialPosts.date
            div.btn_post
      input(type="checkbox", id="btn-mas")
      div.new_post
        a(href="/newPost", class="icon-post") NEW POST
      div.btn-mas
        label(for="btn-mas", class="fa fa-plus")
script(src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.5/flowbite.min.js")
  script(src="https://cdn.tailwindcss.com")